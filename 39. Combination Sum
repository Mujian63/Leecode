class Solution:
    def combinationSum(self, candidates: List[int], target: int) -> List[List[int]]:
        res = []
        nums = sorted(candidates)
        def back(nums,cur):
            if sum(cur)>target:
                return
            if sum(cur) == target:
                res.append(cur)
                return
            
            for i in range(len(nums)):
                back(nums[i:],cur + [nums[i]])

            return res
        return  back(nums,[])
        
